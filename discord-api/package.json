{
  "name": "discord-api",
  "version": "1.0.0",
  "description": "Abstraction of Discord API",
  "main": "index.js",
  "scripts": {
    "test": "cls && jest",
    "compile": "tsc",
    "start": "npm run compile && cls && node dist/index.js"
  },
  "keywords": [
    "Discord",
    "API"
  ],
  "author": "Felipe Matheus Flohr",
  "license": "ISC",
  "dependencies": {
    "discord.js": "^13.7.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.1",
    "jest": "^28.1.0",
    "ts-jest": "^28.0.4",
    "typescript": "^4.7.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "transform": {
      "node_modules/variables/.+\\.(j|t)sx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!variables/.*)"
    ]
  }
}
